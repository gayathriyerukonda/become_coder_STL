#include<bits/stdc++.h>
using namespace std;
int main()
{
	pair<int,int>p1;           //creating pair
	p1.first=3; 
	p1.second=4;
	cout<<p1.second;
	cout<<endl;
    vector<pair<int,int>>p;    //creating vector of pairs
    p.push_back({1,3});
    p.push_back({2,4});
    for(auto i : p)
     {
     	cout<<i.first<<' '<<i.second<<endl; //accessing first and second values of pair of integers
     }
    vector<pair<int,pair<int,string>>>pq; //creating vector of pair of integer,pair
    pq.push_back({1,{2,"gayathri"}});
    pq.push_back({5,{7,"hi"}});
    for(auto x : pq)
     {
      cout<<x.first<<' '<<x.second.first<<' '<<x.second.second<<endl;
     }
     vector<pair<int,pair<int,vector<int>>>>q={{12,{34,{23,78,67,90}}}};
     cout<<q[0].first<<' '<<q[0].second.first<<' '<<q[0].second.second[2]<<endl;
}
